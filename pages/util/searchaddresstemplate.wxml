<!--pages/util/searchaddresstemplate.wxml-->
<template name="searchaddress">
    <scroll-view id="searchaddress" scroll-y enable-back-to-top scroll-top="{{scrollTop}}">
        <view class=" list">
            <view wx:if="{{false}}" class="isNotDataHint">
                <view class="iconfont icon-xiaoxi"></view>
                <view>无法获取地区</view>
            </view>
            <block wx:for="{{addressinfo}}" wx:for-item="item" wx:key="*this">
                <view class="item" bindtap="getAddressPCCList" data-item="{{item}}">
                    {{item.Name}}
                </view>
            </block>
        </view>
    </scroll-view>
</template>
